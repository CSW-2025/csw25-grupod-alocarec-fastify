generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"  // Adicionando o caminho de sa√≠da
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Usuario {
  id    Int     @id @default(autoincrement())
  nome  String
  email String  @unique
}
