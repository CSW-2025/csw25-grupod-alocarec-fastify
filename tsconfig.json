{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
    "baseUrl": ".", 
    "paths": {
      "@usuarios/*": ["src/domains/usuario/*"],
      "@grupo/*": ["src/domains/grupo/*"],
    }
  },
  "include": ["src", "prisma"],
  "exclude": ["node_modules", "dist"]
}